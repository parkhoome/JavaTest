
public class ForTest1 {
public static void main(String[] args) {
	 
	int a;
	 for(a=0; a<3; a++) {
		 System.out.println(a + "  ");
}
	 System.out.println();
	 
	 for(int i=0; i < 3; a++) {
		 System.out.println(i + "");
		 
	 }
	 for(int i =0; i<100; i++) {
		 System.out.println(i + "");
		 
	 }
	 for(int i =10; i<100; i++) {
	 System.out.println();
	 }
	 
			}
		}
