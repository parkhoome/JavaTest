package oep1;

public class Point2D {

	
		/*클래스와 오브젝트
		 * class
		 * object 
		 */
		private int x;  
		private int y;
		
		public int getX() {
			return x;
		}
		public void setX(int x) {
			this.x = x;
		}
		public int getY() {
			return y;
		}
		public void setY(int y) {
			this.y = y;
		}  
	
		

	

}
