package array;

public class ArrayTest2 {
	public static void main(String[] args) {
		
		int[][] n = 
			{
					{10,20,30},
					{40,50,60}
			};
		for (int row=0; row < n.length; row++) {
			for(int col=0; col < n[row].length; col++) {
				System.out.println(row + " ," + col + " :" n[row][col]);
			}
		}
	}

}
