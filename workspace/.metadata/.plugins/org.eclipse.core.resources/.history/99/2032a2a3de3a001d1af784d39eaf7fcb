package operator;

public class OpTest {

	public static void main(String[] args) {
		
		boolean t1,t2;
		t1 = true;  t2 =true;
		System.out.println(t1 && t1);
		System.out.println(t1 && t1);
		System.out.println(t1 && t1);
		System.out.println(t1 && t1);
		System.out.println(t1 && t1);
		
		
		byte a =10, b=5;
		System.out.println(a ^ b);
		
	}
}
