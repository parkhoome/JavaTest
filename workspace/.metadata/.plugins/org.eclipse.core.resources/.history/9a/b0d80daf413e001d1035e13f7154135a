package MethodTest1;

public class MethodTest2 {
 public static void main(String[] args)
print(1);
print(2,3);
print(4,5,6);
}
//1개짜리 변수가 넘어오면 한개 만든다 만약 3개 짜리가 넘어오면 
private static void print(int...i) {
	for( int j =0; j<i.length; j++) {
	System.out.println(i[j + "\t"]);
	}
	System.out.println();

}
}